
<h2> Name: <%= @product.name %> </h2>
<h2> Price: <%= @product.price %></h2>
<h3> Description: <%= @product.description %></h3>
<h4> Supplier Name: <%= @product.supplier.name %></h4>

<div>
  <h3>Images: </h3>
      <% @product.images.each do |image|%>
      
      <img src="<%= image.url %>">   

      <% end %>
    
  </div>
    
 

<a href="/products/<%= @product.id%>/edit">Edit</a>

  

